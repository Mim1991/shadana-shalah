<%= simple_form_for(blog) do |f| %>
    <%= f.input :title %>
    <%= f.rich_text_area :rich_body %>
    <%= f.input :synopsis %>
    <%= f.input :category %>
    <%= f.input :published_date %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.button :submit, class:"btn btn-primary" %>
<% end %>
